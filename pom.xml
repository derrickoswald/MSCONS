        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <version.dependency.junit>4.11</version.dependency.junit>
        <version.dependency.scala>2.11</version.dependency.scala>
        <version.dependency.scalalibrary>2.11.12</version.dependency.scalalibrary>
        <version.dependency.parser-combinators>1.1.2</version.dependency.parser-combinators>
        <version.dependency.exec-maven-plugin>1.6.0</version.dependency.exec-maven-plugin>
        <version.dependency.slf4j>1.7.30</version.dependency.slf4j>
        <version.dependency.maven-failsafe-plugin>2.22.2</version.dependency.maven-failsafe-plugin>
        <version.dependency.maven-surefire-plugin>3.0.0-M3</version.dependency.maven-surefire-plugin>
        <version.dependency.scalatest>3.0.1</version.dependency.scalatest>
        <version.dependency.scalamavenplugin>4.3.1</version.dependency.scalamavenplugin>
        <version.dependency.scopt>3.5.0</version.dependency.scopt>
