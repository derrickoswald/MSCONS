<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>ch.ninecode.mscons</groupId>
    <artifactId>MSCONS</artifactId>
    <!-- version>${version.dependency.scala}-${version.dependency.spark}-MSCONSREADER_VERSION</version -->
    <version>2.11-2.0.2-0.0.2</version>
    <packaging>pom</packaging>
    <name>${project.artifactId}</name>
    <description>Read and write MSCONS data files</description>
    <inceptionYear>2016</inceptionYear>
    <url>https://github.com/derrickoswald/MSCONS</url>
    <organization>
        <name>9code GmbH</name>
        <url>http://9code.ch</url>
    </organization>
    <scm>
        <url>https://github.com/derrickoswald/MSCONS</url>
    </scm>
    <issueManagement>
        <url>https://github.com/derrickoswald/MSCONS</url>
    </issueManagement>

    <properties>
